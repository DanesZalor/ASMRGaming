; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="ASMR Robotgame"
run/main_scene="res://Robot/Robot.tscn"
config/icon="res://icon.png"

[autoload]

RAM="*res://MachineCodeSimulatorClassFiles/CPU/RAM.cs"
ALU="*res://MachineCodeSimulatorClassFiles/CPU/ALU.cs"
CPU="*res://MachineCodeSimulatorClassFiles/CPU/CPU.cs"
Lexicon="*res://MachineCodeSimulatorClassFiles/Assembler/LEXICON.cs"
Common="*res://MachineCodeSimulatorClassFiles/Assembler/Common.cs"
PreprocessorDirectives="*res://MachineCodeSimulatorClassFiles/Assembler/PreprocessorDirectives.cs"
SyntaxChecker="*res://MachineCodeSimulatorClassFiles/Assembler/SyntaxChecker.cs"
Translator="*res://MachineCodeSimulatorClassFiles/Assembler/Translator.cs"
Assembler="*res://MachineCodeSimulatorClassFiles/Assembler/Assembler.cs"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
